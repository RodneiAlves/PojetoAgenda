<% if (erro.length>0) { %>
    <div class="row">
      <div class="col">
        <div class="alert alert-danger">
          <% erro.forEach(erro =>{ %>
           <%=erro%>
         <% })%>
        </div>
      </div>
    </div>
  <% } %>

  <% if (success.length>0) { %>
    <div class="row">
      <div class="col">
        <div class="alert alert-success">
          <% success.forEach(success =>{ %>
           <%=success%>
         <% })%>
        </div>
      </div>
    </div>
  <% } %>